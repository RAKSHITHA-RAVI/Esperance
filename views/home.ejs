            <input type="text" id="form1" class="form-control validate">
          </div>
  
          <div class="md-form">
            <i class="fa fa-envelope prefix grey-text"> 
               <label data-error="wrong" data-success="right" for="form2">Your email</label></i>
            <input type="email" id="form2" class="form-control validate">
          </div>
        </form>
      </div>
      <!--Footer-->
      <div class="modal-footer justify-content-center">
        <a type="button" class="btn btn-outline-warning waves-effect">Send <i class="fa fa-paper-plane-o ml-1"></i></a>
      </div>
    </div>
  </div>
</div>

